

public class DS4_LinkedList<E> implements DS4_LinkedList_Interface<E>
{

    DS4_LinkedList_Node<E> head= new DS4_LinkedList_Node<E>(null);
    DS4_LinkedList_Node<E> tail= new DS4_LinkedList_Node<E>(null);



    @Override
    public DS4_LinkedList_Node<E> getFirstNode()
    {
        return head;

    }

    @Override
    public DS4_LinkedList_Node<E> getLastNode()
    {
        return tail;
    }

    @Override
    public E getFirst()
    {
        return head.getData();

    }

    @Override
    public E getLast()
    {
        return tail.getData();
    }

    @Override
    public E removeFirst()
    {
        DS4_LinkedList_Node<E> newFirstNode= head.getNext();
        E value= head.getData();
        head=newFirstNode;
         return value;

    }

    @Override
    public E removeLast()
    {
        DS4_LinkedList_Node<E> newLastNode = new DS4_LinkedList_Node<>(null);
        newLastNode=head;
        while (newLastNode.getNext()!=tail)
        {
            newLastNode=newLastNode.getNext();
        }
        //by now the node should have its next node be last
        E value= tail.getData();
        tail=newLastNode;
        return value;


    }

    @Override
    public void addFirst(E data)
    {
        DS4_LinkedList_Node<E> newHead= new DS4_LinkedList_Node<>(null);

        newHead.setData(data);

        DS4_LinkedList_Node<E> newHeadNext= new DS4_LinkedList_Node<>(null);

        newHeadNext=head;

        head=newHead;

        head.setNext(newHeadNext);




    }

    @Override
    public void addLast(Object data)
    {
        DS4_LinkedList_Node<E> newNode= new DS4_LinkedList_Node<>(null);
        newNode.setData((E) data);
        tail.setNext(newNode);
    }

    @Override
    public void clear()
    {
        head=null;
        tail=null;

    }

    @Override
    public int size()
    {
        int count=0;
        DS4_LinkedList_Node<E> nodeCycle= new DS4_LinkedList_Node<>(null);
        nodeCycle=head;
        while (nodeCycle!=null)
        {
            count++;
            nodeCycle=nodeCycle.getNext();
        }
        return count;
    }

    @Override
    public E get(int x)
    {
        DS4_LinkedList_Node<E> nodeCycle= new DS4_LinkedList_Node<>(null);
        nodeCycle=head;
        for (int i=0; i<x;i++)
        {
            nodeCycle=nodeCycle.getNext();
        }
        // by now nodecycle should be EQUAL to index x
        return nodeCycle.getData();

    }

    @Override
    public void add(int x, Object data) {

    }

    @Override
    public E remove(int x) {
        return null;
    }

    @Override
    public E set(int x, E data) {
        return null;
    }

    @Override
    public boolean isEmpty()
    {
        if (head==null&&tail==null)
        {
            return true;
        }
        else
            return false;
    }
    public String toString()
    {
        if (isEmpty())
        {
            return "";
        }

        DS4_LinkedList_Node<E> nodeCycle= new DS4_LinkedList_Node<>(null);
        nodeCycle=head;
        String s="[";
        while (true)
        {
            if (nodeCycle.getNext()==null)
            {
                s+=nodeCycle.getData()+"]";
                return s;

            }
            else
            {
               s+=nodeCycle+", ";
            }
            nodeCycle=nodeCycle.getNext();
        }
    }
}
